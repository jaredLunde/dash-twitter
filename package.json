{
  "name": "poc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-storybook": "build-storybook -s ./public",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "test": "jest --passWithNoTests",
    "start": "next build && next start",
    "storybook": "start-storybook -s ./public -p 6006",
    "validate": "yarn check-types && yarn lint && yarn test -- --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,html,md,mdx,yml,json}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx,js,jsx}": [
      "pnpm lint -- --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "eslintIgnore": [
    "coverage",
    ".storybook",
    ".next",
    "build",
    "public",
    "test",
    "storybook-static",
    "/*.js"
  ],
  "dependencies": {
    "@dash-ui/compound": "^0.1.1",
    "@dash-ui/jest": "^2.1.2",
    "@dash-ui/layout": "^0.1.0",
    "@dash-ui/mq": "^0.4.1",
    "@dash-ui/react": "^0.9.1",
    "@dash-ui/reset": "^0.3.1",
    "@dash-ui/responsive": "^0.2.3",
    "@dash-ui/styles": "^0.8.6",
    "@dash-ui/transition": "^0.4.0",
    "@hookform/resolvers": "^2.8.0",
    "@next/plugin-storybook": "11.1.1-canary.17",
    "@radix-ui/react-alert-dialog": "^0.0.20",
    "@radix-ui/react-checkbox": "^0.0.17",
    "@radix-ui/react-dialog": "^0.0.20",
    "@radix-ui/react-dropdown-menu": "^0.0.23",
    "@radix-ui/react-hover-card": "^0.0.4",
    "@radix-ui/react-id": "^0.0.6",
    "@radix-ui/react-popover": "^0.0.20",
    "@radix-ui/react-radio-group": "^0.0.19",
    "@radix-ui/react-slider": "^0.0.17",
    "@radix-ui/react-switch": "^0.0.15",
    "@radix-ui/react-tabs": "^0.0.16",
    "@radix-ui/react-tooltip": "^0.0.21",
    "@react-hook/merged-ref": "^1.3.0",
    "@react-hook/window-size": "^3.0.7",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/builder-webpack5": "^6.3.7",
    "@storybook/manager-webpack5": "^6.4.0-alpha.32",
    "@storybook/react": "^6.3.7",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "latest",
    "@testing-library/user-event": "^12.1.1",
    "@types/react": "17.0.19",
    "clsx": "^1.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.32.0",
    "eslint-config-lunde": "^0.4.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "husky": "^4.3.8",
    "immer": "^9.0.5",
    "jest": "^27.0.6",
    "jotai": "^1.3.1",
    "lint-staged": "^10.5.3",
    "next": "11.1.0",
    "next-image-loader": "^2.0.4",
    "prettier": "^2.2.1",
    "qs-stringify": "^1.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.13.0",
    "storybook-addon-next-router": "^3.0.7",
    "storybook-addon-themes": "^6.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "4.3.5",
    "yup": "^0.32.9",
    "zustand": "^3.5.9"
  }
}
